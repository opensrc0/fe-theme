{
  "name": "fe-theme",
  "version": "2.20.0",
  "description": "fe-theme",
  "main": "babel.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release",
    "build": "run-s build:clean && run-p build:es",
    "build:clean": "rimraf es cjs",
    "build:es": "babel js -d es --ignore *.test.js",
    "build:prepare": "node ./js/scripts/buildPrepare.js",
