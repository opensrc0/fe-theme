    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
