{
  "name": "fe-theme",
  "version": "4.1.7",
  "description": "fe-theme",
  "main": "babel.config.js",
  "scripts": {
    "test": "echo \"Success: Verified\"",
    "start": "npm run local",
    "local": "babel __application/component -d __build-es --watch",
    "build": "run-s build-es build-prepare",
    "prebuild-es": "echo \"\\x1b[97m\\x1b[44m 1. Started: Building Component in __build-es folder. \\x1b[0m\"",
    "build-es": "babel __application/component -d __build-es --ignore *.test.js",
    "prebuild-prepare": "echo \"\\x1b[97m\\x1b[44m 2. Started: Direct import of individual package preparation started. \\x1b[0m\"",
    "build-prepare": "node ./__application/scripts/buildPrepare.js",
    "theme-prepare": "node -r esm ./__appset/themePrepare.js",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "version-i": "npm version patch",
    "storybook": "storybook dev -p 8888",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release --branches master"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "devEngines": {
    "node": "16.x || 18.x || 19.x || 20.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensrc0/fe-theme.git"
  },
  "author": "Himanshu Gupta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opensrc0/fe-theme/issues"
  },
  "homepage": "https://github.com/opensrc0/fe-theme#readme",
  "keywords": [
    "accessible",
    "css",
    "components",
    "design-system",
    "emotion",
    "front-end",
    "framework",
    "library",
    "mobile-first",
    "react",
    "react-components",
    "responsive",
    "styled-components",
    "theme",
    "theming",
    "ui",
    "ui-component",
    "ui-design",
    "ui-design-system",
    "ui-library",
    "web"
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-external-helpers": "^7.23.3",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-flow": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-webpack5": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "storybook": "^7.6.4",
    "styled-components": "^6.1.12",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.6"
  },
  "files": [
    "__application/utils/**",
    "__build-es/**",
    "__appset/**",
    "Box/",
    "Button/",
    "Flex/",
    "Input/",
    "theme/",
    "init.js"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.5.1",
    "esm": "^3.2.25",
    "lodash.merge": "^4.6.2",
    "mkdirp": "^3.0.1",
    "stylis": "^4.3.2"
  }
}
